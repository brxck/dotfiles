{
  "workbench.colorTheme": "Ayu Mirage Bordered",
  "explorer.confirmDelete": false,
  "editor.tabSize": 2,
  "editor.fontFamily": "'Fira Code'",
  "editor.fontSize": 18,
  "editor.fontLigatures": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.rulers": [80],
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#31313b"
  },
  "terminal.integrated.fontSize": 16,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.shellArgs.linux": ["--login"],
  "git.enableSmartCommit": true,
  "workbench.sideBar.location": "left",
  "editor.minimap.enabled": false,
  "workbench.activityBar.visible": false,
  "scss.lint.boxModel": "warning",
  "scss.lint.compatibleVendorPrefixes": "warning",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.float": "warning",
  "scss.lint.important": "warning",
  "scss.lint.universalSelector": "warning",
  "scss.lint.zeroUnits": "warning",
  "metaGo.decoration.backgroundColor": "darkslategray,darkslategray",
  "metaGo.decoration.fontSize": 16,
  "metaGo.decoration.fontFamily": "Fira Mono",
  "metaGo.decoration.height": 16,
  "metaGo.decoration.width": 14,
  "metaGo.decoration.backgroundOpacity": "1",
  "metaGo.decoration.borderColor": "slategray",
  "metaGo.decoration.color": "lightgray",
  "metaGo.decoration.x": 2,
  "metaGo.decoration.y": 14,
  "metaGo.decoration.fontWeight": "regular",
  "metaGo.decoration.characters": "a, b, c, d, e, f, g, h, i, j, k, l, m, n, o, p, q, r, s, t, u, w, x, y, z",
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "solargraph.diagnostics": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "solargraph.formatting": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.formatOnSave": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.titleBarStyle": "custom",
  "terminal.integrated.rendererType": "canvas",
  "terminal.external.linuxExec": "tilda",
  "prettier.eslintIntegration": true,
  "javascript.validate.enable": false,
  "css.validate": false,
  "scss.validate": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "javascript.format.enable": false,
  "prettier.trailingComma": "es5",
  "bracket-pair-colorizer-2.excludedLanguages": ["python"],
  "bracket-pair-colorizer-2.colors": [
    "#e06c75",
    "#d19a66",
    "#E5C07B",
    "#61AFEF",
    "#98c379",
    "#56b6c2"
  ],
  "editor.lineNumbers": "relative",
  "editor.smoothScrolling": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
  "zenMode.hideTabs": false,
  "npm.enableScriptExplorer": true,
  "npm.scriptExplorerAction": "run",
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "python -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runhaskell",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "sml": "rlwrap /usr/share/sml/bin/sml $fullFileName"
  },
  "code-runner.runInTerminal": true,
  "scm.alwaysShowProviders": true,
  "sml.ignoreMissingSmlDotJson": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false
}
